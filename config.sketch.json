{
    "proxy": {
        "listen": "127.0.0.1:8100",
        "target": "127.0.0.1:8080",
        "response": {
            "headers": {
                "server": {
                    "override": true,
                    "version": true
                },
                "via": {
                    "extend": true
                }
            }
        },
        "request": {
            "headers": {
                "forward": {
                    "extend": true
                }
            }
        }
    }
}
